{
  "name": "onkeltem/yaklass-progress",
  "description": "Yaklass progress data fetcher and analyser",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Artiom Neganov",
      "email": "aneganov@gmail.com",
      "role": "Developer"
    }
  ],
  "type": "package",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php-webdriver/webdriver": "^1.7.1",
    "league/csv": "^9.4",
    "datamincer/task-runner": "^0.0.5",
    "google/apiclient": "^2.0",
    "doctrine/dbal": "^2.9",
    "psr/log": "^1.0.1",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-json": "*",
    "ext-intl": "*",
    "ext-pdo": "*",
    "ext-mbstring": "*"
  },
  "bin": [
    "bin/yaklass-progress",
    "bin/yaklass-progress-install.sh",
    "bin/yaklass-progress-start.sh",
    "bin/yaklass-progress-stop.sh"
  ],
  "autoload": {
    "psr-4": {
      "Yaklass\\": "src/"
    },
    "files": [
      "src/TaskSync.php",
      "src/TaskList.php",
      "src/TaskTestLoad.php",
      "src/TaskPublish.php"
    ]
